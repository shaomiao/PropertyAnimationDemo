<?xml version="1.0" encoding="utf-8"?><FrameLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/activity_animation_popup_menu"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context="com.example.shaomiao.myapplication.AnimationPopupMenuActivity">    <ImageView        android:id="@+id/b"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:src="@mipmap/b"        android:paddingTop="5dp"        android:paddingLeft="5dp"        />    <ImageView        android:id="@+id/c"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:src="@mipmap/c"        android:paddingTop="5dp"        android:paddingLeft="5dp"        />    <ImageView        android:id="@+id/d"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:src="@mipmap/d"        android:paddingTop="5dp"        android:paddingLeft="5dp"        />    <ImageView        android:id="@+id/e"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:src="@mipmap/e"        android:paddingTop="5dp"        android:paddingLeft="5dp"    />    <ImageView        android:id="@+id/f"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:src="@mipmap/f"        android:paddingTop="5dp"        android:paddingLeft="5dp"        />    <ImageView        android:id="@+id/g"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:src="@mipmap/g"        android:paddingTop="5dp"        android:paddingLeft="5dp"        />    <ImageView        android:id="@+id/h"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:src="@mipmap/h"      